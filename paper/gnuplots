reset
set notitle
set xlabel "Number of route processors"
set ylabel "Events per second (Millions)"
set yrange [0:0.7]
set xtics 1
set term png
set output "throughput_route.png"
plot  "local_route.data" using 1:3 title 'Single JVM (Multi threaded)' lw 3 with linespoints, "remote_route.data" using 1:3 title 'Distributed' lw 3 with linespoints


reset
set notitle
set xlabel "Number of route processors"
set ylabel "Mean latency (ms)"
set yrange [0:95]
set xtics 1
set term png
set output "latency_route.png"
plot  "local_route.data" using 1:2 title 'Single JVM (Multi threaded)' lw 3 with linespoints, "remote_route.data" using 1:2 title 'Distributed' lw 3 with linespoints

reset
set notitle
set xlabel "Number of profit calculators"
set ylabel "Events per second (Millions)"
set yrange [0:0.45]
set xtics 1
set term png
set output "throughput_profit.png"
plot  "local_profit.data" using 1:3 title 'Single JVM (Multi threaded)' lw 3 with linespoints, "remote_profit.data" using 1:3 title 'Distributed' lw 3 with linespoints

reset
set notitle
set xlabel "Number of profit calculators"
set ylabel "Mean latency (ms)"
set yrange [0:200]
set xtics 1
set term png
set output "latency_profit.png"
plot  "local_profit.data" using 1:2 title 'Single JVM (Multi threaded)' lw 3 with linespoints, "remote_profit.data" using 1:2 title 'Distributed' lw 3 with linespoints

reset
set notitle
set xlabel "Window size (hours)"
set ylabel "Events per Second (Millions)"
set yrange [0:0.24]
set xtics 4
set term png
set output "window.png"
plot  "window.data" using 1:2 title 'PriorityQueue based' lw 3 with linespoints, "window.data" using 1:3 title 'DynamicHeap based' lw 3 with linespoints
