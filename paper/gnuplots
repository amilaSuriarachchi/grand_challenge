reset
set notitle
set xlabel "Number of route processors"
set ylabel "Execution time (Min)"
set yrange [0:18]
set xtics 1
set term png
set output "time_route.png"
plot  "local_route.data" using 1:2 title 'Single JVM' lw 2 with linespoints, "remote_route.data" using 1:2 title 'Distributed' lw 2 with linespoints


reset
set notitle
set xlabel "Number of route processors"
set ylabel "Latency (ms)"
set yrange [0:100]
set xtics 1
set term png
set output "latency_route.png"
plot  "local_route.data" using 1:3 title 'Single JVM' lw 2 with linespoints, "remote_route.data" using 1:3 title 'Distributed' lw 2 with linespoints

reset
set notitle
set xlabel "Number of profit calculators"
set ylabel "Execution time (Min)"
set yrange [0:55]
set xtics 1
set term png
set output "time_profit.png"
plot  "local_profit.data" using 1:2 title 'Single JVM' lw 2 with linespoints, "remote_profit.data" using 1:2 title 'Distributed' lw 2 with linespoints

reset
set notitle
set xlabel "Number of profit calculators"
set ylabel "Latency (ms)"
set yrange [0:220]
set xtics 1
set term png
set output "latency_profit.png"
plot  "local_profit.data" using 1:3 title 'Single JVM' lw 2 with linespoints, "remote_profit.data" using 1:3 title 'Distributed' lw 2 with linespoints

